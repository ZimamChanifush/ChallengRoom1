<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".SiswaAdapter">

    <TextView
        android:id="@+id/t_nama"
        android:layout_width="410dp"
        android:layout_height="32dp"
        android:layout_marginTop="32dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageEdit"
        android:layout_width="28dp"
        android:layout_height="30dp"
        android:layout_marginStart="284dp"
        app:layout_constraintStart_toStartOf="@+id/t_nama"
        app:layout_constraintTop_toTopOf="@+id/t_nama"
        app:srcCompat="@drawable/ic_edit" />

    <ImageView
        android:id="@+id/imageDelete"
        android:layout_width="31dp"
        android:layout_height="30dp"
        app:layout_constraintEnd_toEndOf="@+id/t_nama"
        app:layout_constraintStart_toEndOf="@+id/imageEdit"
        app:layout_constraintTop_toTopOf="@+id/t_nama"
        app:srcCompat="@drawable/ic_delete" />

</androidx.constraintlayout.widget.ConstraintLayout>